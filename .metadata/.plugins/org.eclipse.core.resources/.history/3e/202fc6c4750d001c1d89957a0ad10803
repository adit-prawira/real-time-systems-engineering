#define BUF_SIZE 100

typedef union{ // replacing the standard "union sigval" with our own _customSigval
	union{
		_Uint32t sival_int;
		void *sival_ptr; // will have different size of 32-bit and 64-bit
	};
	_Uint32t dummyu[4]; // Use the dummy variable to create space
}_customSigval;


