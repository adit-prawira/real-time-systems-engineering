#include <stdio.h>
#include <stdlib.h>
#include <errno.h>
#include <sys/dispatch.h>
#define LOCAL_ATTACH_POINT "slogger2"
//#define QNET_ATTACH_POINT  "/net/VM_x86_Target01/dev/name/local/slogger2"
//#define GLOBAL_ATTACH_POINT  "/net/VM_x86_Target01/dev/name/global/myname"

#define BUF_SIZE 100

typedef union{ // replacing the standard "union sigval" with our own _customSigval
	union{
		_Uint32t sival_int;
		void *sival_ptr; // will have different size of 32-bit and 64-bit
	};
	_Uint32t dummyu[4]; // Use the dummy variable to create space
}_customSigval;

typedef struct _CustomPulse{
	_Uint16t type;
	_Uint16t subtype;
	_Int8t code;
	_Uint8t zero[3];
	_customSigval value;
	_Uint8t zero2[2];
	_Int32t serverConnectionId;
}msg_header_t;

typedef struct{
	msg_header_t header; // Custom header
	int clientId;
	itn data;
}messageData;

typedef struct {
	msg_header_t header;
	char buf[BUF_SIZE];
}reply;

int client(char *serverName);

int main(void) {
	puts("Hello World!!!"); /* prints Hello World!!! */
	return EXIT_SUCCESS;
}
